<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" fx:id="pane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="guiprodfournisseur.AjoutProduitController">
   <children>
      <Label fx:id="welcome" alignment="CENTER" layoutX="185.0" layoutY="41.0" text="Ajouter un produit">
         <font>
            <Font size="28.0" />
         </font></Label>
      <TextField fx:id="nomProd" layoutX="101.0" layoutY="108.0" prefHeight="26.0" prefWidth="408.0" promptText="Désignation" AnchorPane.leftAnchor="101.0" AnchorPane.rightAnchor="91.0" />
      <TextField fx:id="quantite" layoutX="99.0" layoutY="151.0" prefHeight="26.0" prefWidth="408.0" promptText="Quantité" AnchorPane.leftAnchor="101.0" AnchorPane.rightAnchor="91.0" />
      <TextField fx:id="prix" layoutX="101.0" layoutY="200.0" prefHeight="26.0" prefWidth="408.0" promptText="Prix" AnchorPane.leftAnchor="101.0" AnchorPane.rightAnchor="91.0" />
      <Button fx:id="ajouter" layoutX="106.0" layoutY="348.0" mnemonicParsing="false" onAction="#insert" prefHeight="26.0" prefWidth="78.0" text="Ajouter" />
      <Button fx:id="annuler" layoutX="421.0" layoutY="349.0" mnemonicParsing="false" onAction="#annuler" text="Annuler" />
      <ImageView fx:id="image" fitHeight="90.0" fitWidth="110.0" layoutX="399.0" layoutY="241.0" pickOnBounds="true" preserveRatio="true" />
      <Button fx:id="btnImage" layoutX="101.0" layoutY="260.0" mnemonicParsing="false" onAction="#insertImage" text="Selectionner une image" />
   </children>
</AnchorPane>
