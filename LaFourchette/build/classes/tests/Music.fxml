<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane id="AnchorPane" fx:id="pane" prefHeight="122.0" prefWidth="464.0" style="-fx-background-color: ff8669;" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/17" fx:controller="tests.MusicController">
   <children>
      <Label fx:id="songLabel" alignment="CENTER" contentDisplay="CENTER" prefHeight="38.0" prefWidth="463.0" text="Music" textFill="#f8f8f1">
         <font>
            <Font size="19.0" />
         </font>
      </Label>
      <ProgressBar fx:id="songProgressBar" layoutY="38.0" prefHeight="18.0" prefWidth="463.0" progress="0.0" />
      <Button fx:id="playButton" layoutX="1.0" layoutY="61.0" mnemonicParsing="false" onAction="#playMedia" prefHeight="25.0" prefWidth="73.0" text="PLAY" textFill="#ef451b" />
      <Button fx:id="pauseButton" layoutX="77.0" layoutY="61.0" mnemonicParsing="false" onAction="#pauseMedia" prefHeight="25.0" prefWidth="73.0" text="PAUSE" textFill="#ef451b" />
      <Button fx:id="resetButton" layoutX="153.0" layoutY="61.0" mnemonicParsing="false" onAction="#resetMedia" prefHeight="25.0" prefWidth="73.0" text="RESET" textFill="#ef451b" />
      <Button fx:id="previousButton" layoutX="229.0" layoutY="61.0" mnemonicParsing="false" onAction="#previousMedia" prefHeight="25.0" prefWidth="73.0" text="PREVIOUS" textFill="#ef451b" />
      <Button fx:id="nextButton" layoutX="305.0" layoutY="61.0" mnemonicParsing="false" onAction="#nextMedia" prefHeight="25.0" prefWidth="73.0" text="NEXT" textFill="#ef451b" />
      <ComboBox fx:id="speedBox" layoutX="381.0" layoutY="61.0" onAction="#changeSpeed" prefHeight="25.0" prefWidth="81.0" promptText="SPEED" />
      <Slider fx:id="volumeSlider" layoutX="7.0" layoutY="94.0" prefHeight="14.0" prefWidth="451.0" value="50.0" />
   </children>
</AnchorPane>
