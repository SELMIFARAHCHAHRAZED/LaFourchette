<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" fx:id="pane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="guiprodfournisseur.ModifierProduitController">
   <children>
      <Label fx:id="welcome" layoutX="178.0" layoutY="32.0" text="Modifier un produit">
         <font>
            <Font size="28.0" />
         </font></Label>
      <TextField fx:id="nomProd" disable="true" layoutX="96.0" layoutY="88.0" prefHeight="26.0" prefWidth="408.0" promptText="Désignation" AnchorPane.leftAnchor="96.0" AnchorPane.rightAnchor="96.0" />
      <TextField fx:id="quantite" layoutX="96.0" layoutY="135.0" prefHeight="26.0" prefWidth="408.0" promptText="Quantité" AnchorPane.leftAnchor="96.0" AnchorPane.rightAnchor="96.0" />
      <TextField fx:id="prix" layoutX="96.0" layoutY="187.0" prefHeight="26.0" prefWidth="408.0" promptText="Prix" AnchorPane.leftAnchor="96.0" AnchorPane.rightAnchor="96.0" />
      <Button fx:id="modifier" layoutX="154.0" layoutY="336.0" mnemonicParsing="false" onAction="#modif" text="Modifier" />
      <Button fx:id="annuler" layoutX="363.0" layoutY="336.0" mnemonicParsing="false" onAction="#annuler" text="Annuler" />
      <ImageView fx:id="image" fitHeight="95.0" fitWidth="127.0" layoutX="377.0" layoutY="227.0" pickOnBounds="true" preserveRatio="true" />
      <Button fx:id="btnImage" layoutX="96.0" layoutY="262.0" mnemonicParsing="false" onAction="#insertImage" text="Selectionner une image" />
   </children>
</AnchorPane>
